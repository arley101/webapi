{
  "manifestVersion": "1.16",
  "version": "1.0.0",
  "id": "elite-dynamics-ai-bot",
  "packageName": "com.elitedynamics.assistant",
  "developer": {
    "name": "Elite Dynamics",
    "websiteUrl": "https://elitedynamicsapi.azurewebsites.net",
    "privacyUrl": "https://elitedynamicsapi.azurewebsites.net/privacy",
    "termsOfUseUrl": "https://elitedynamicsapi.azurewebsites.net/terms"
  },
  "name": {
    "short": "EliteDynamics AI",
    "full": "Asistente Empresarial EliteDynamics"
  },
  "description": {
    "short": "Asistente inteligente con 466+ acciones empresariales",
    "full": "Automatiza tu empresa con workflows de backup, marketing, contenido, YouTube y onboarding de clientes"
  },
  "bots": [
    {
      "botId": "TU_BOT_ID_DE_AZURE_AQUI",
      "scopes": [
        "personal",
        "team",
        "groupchat"
      ],
      "commandLists": [
        {
          "scopes": [
            "personal",
            "team",
            "groupchat"
          ],
          "commands": [
            {
              "title": "Lista workflows",
              "description": "Ver workflows disponibles"
            },
            {
              "title": "Ejecuta backup",
              "description": "Backup completo"
            },
            {
              "title": "Sincroniza marketing",
              "description": "Dashboard marketing"
            }
          ]
        }
      ]
    }
  ],
  "permissions": [
    "identity",
    "messageTeamMembers"
  ],
  "validDomains": [
    "elitedynamicsapi.azurewebsites.net"
  ]
}