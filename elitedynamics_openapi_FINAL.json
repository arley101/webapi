{
  "openapi": "3.1.1",
  "info": {
    "title": "EliteDynamicsAPI",
    "description": "API empresarial con 476+ integraciones - Rutas reales del backend",
    "version": "1.1"
  },
  "servers": [
    {
      "url": "https://elitedynamicsapi.azurewebsites.net",
      "description": "Servidor de Producción"
    }
  ],
  "paths": {
    "/api/v1/dynamics": {
      "post": {
        "summary": "Ejecutar acción dinámica",
        "description": "RUTA PRINCIPAL - Ejecuta cualquiera de las 476+ acciones disponibles (Runway ML, Microsoft Graph, Google Ads, Meta Ads, OpenAI, Notion, HubSpot, WordPress, etc.)",
        "operationId": "executeAction",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["action"],
                "properties": {
                  "action": {
                    "type": "string",
                    "description": "Nombre de la acción a ejecutar",
                    "examples": [
                      "runway_list_models",
                      "runway_generate_video", 
                      "runway_get_video_status",
                      "email_send_message",
                      "calendario_create_event",
                      "notion_create_page",
                      "openai_generate_text",
                      "google_ads_get_campaigns",
                      "meta_ads_get_insights",
                      "wp_create_post",
                      "hubspot_create_contact",
                      "teams_send_message",
                      "youtube_channel_get_videos",
                      "powerbi_get_reports"
                    ]
                  },
                  "params": {
                    "type": "object",
                    "description": "Parámetros específicos para la acción",
                    "additionalProperties": true
                  }
                }
              },
              "examples": {
                "runway_list_models": {
                  "summary": "Listar modelos de Runway ML",
                  "value": {
                    "action": "runway_list_models",
                    "params": {}
                  }
                },
                "runway_generate_video": {
                  "summary": "Generar video con Runway ML",
                  "value": {
                    "action": "runway_generate_video",
                    "params": {
                      "prompt": "A beautiful sunset over calm ocean waters",
                      "model": "gen3a_turbo",
                      "duration": 5,
                      "ratio": "1280:720"
                    }
                  }
                },
                "runway_get_video_status": {
                  "summary": "Verificar estado de video",
                  "value": {
                    "action": "runway_get_video_status",
                    "params": {
                      "task_id": "task_abc123def456"
                    }
                  }
                },
                "email_send_message": {
                  "summary": "Enviar email",
                  "value": {
                    "action": "email_send_message",
                    "params": {
                      "to": ["destinatario@empresa.com"],
                      "subject": "Reporte mensual",
                      "body": "Adjunto el reporte solicitado"
                    }
                  }
                },
                "calendario_create_event": {
                  "summary": "Crear evento en calendario",
                  "value": {
                    "action": "calendario_create_event",
                    "params": {
                      "subject": "Reunión de equipo",
                      "start": "2025-08-20T14:00:00Z",
                      "end": "2025-08-20T15:00:00Z",
                      "attendees": ["usuario@empresa.com"]
                    }
                  }
                },
                "notion_create_page": {
                  "summary": "Crear página en Notion",
                  "value": {
                    "action": "notion_create_page",
                    "params": {
                      "title": "Nueva página de proyecto",
                      "parent_id": "database_id_here",
                      "content": "Contenido inicial"
                    }
                  }
                },
                "openai_generate_text": {
                  "summary": "Generar texto con OpenAI",
                  "value": {
                    "action": "openai_generate_text",
                    "params": {
                      "prompt": "Escribe un artículo sobre IA",
                      "model": "gpt-4",
                      "max_tokens": 1500
                    }
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Acción ejecutada exitosamente",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "enum": ["success"]
                    },
                    "data": {
                      "type": "object",
                      "description": "Datos de respuesta específicos de la acción"
                    },
                    "task_id": {
                      "type": "string",
                      "description": "ID de tarea para operaciones asíncronas"
                    },
                    "models": {
                      "type": "array",
                      "description": "Lista de modelos (runway_list_models)"
                    },
                    "video_url": {
                      "type": "string",
                      "description": "URL del video generado"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Error en la solicitud"
          },
          "500": {
            "description": "Error interno del servidor"
          }
        }
      }
    },
    "/api/v1/jobs/{job_id}": {
      "get": {
        "summary": "Estado de trabajo asíncrono",
        "description": "Consulta el estado de un trabajo asíncrono en ejecución",
        "operationId": "getJobStatus",
        "parameters": [
          {
            "name": "job_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "ID del trabajo asíncrono"
          }
        ],
        "responses": {
          "200": {
            "description": "Estado del trabajo",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "status": {
                      "type": "string",
                      "enum": ["pending", "running", "completed", "failed"]
                    },
                    "result": {
                      "type": "object",
                      "description": "Resultado cuando está completado"
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Trabajo no encontrado"
          }
        }
      }
    },
    "/api/v1/session/start": {
      "post": {
        "summary": "Iniciar sesión de asistente inteligente",
        "description": "Inicia una nueva sesión con el asistente inteligente",
        "operationId": "startIntelligentSession",
        "requestBody": {
          "required": false,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "user_id": {
                    "type": "string"
                  },
                  "context": {
                    "type": "object"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Sesión iniciada"
          }
        }
      }
    },
    "/api/v1/status": {
      "get": {
        "summary": "Estado del asistente",
        "description": "Obtiene el estado actual del asistente inteligente",
        "operationId": "getAssistantStatus",
        "responses": {
          "200": {
            "description": "Estado del asistente"
          }
        }
      }
    },
