        "operationId": "autoSelector",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "query": {
                    "type": "string"
                  },
                  "context": {
                    "type": "object"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Asistente seleccionado"
          }
        }
      }
    },
    "/api/v1/selector/manual": {
      "post": {
        "summary": "Selector manual de asistente",
        "description": "Permite selección manual del asistente",
        "operationId": "manualSelector",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "assistant_type": {
                    "type": "string"
                  },
                  "query": {
                    "type": "string"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Asistente seleccionado manualmente"
          }
        }
      }
    },
    "/api/v1/selector/compare": {
      "get": {
